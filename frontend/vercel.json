{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "${NEXT_PUBLIC_API_URL}/:path*"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://proppulse-ai-production.up.railway.app"
  }
}
