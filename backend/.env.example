# PropPulse AI Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Database
DATABASE_URL=postgresql://username:password@localhost/proppulse_ai

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_service_key

# AI API Keys
GEMINI_API_KEY=your_gemini_api_key_here
RAPIDAPI_KEY=your_rapidapi_key_here

# Email Configuration (Optional)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_EMAIL=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# External APIs (Optional - Free alternatives available)
COSTAR_API_KEY=optional_costar_api_key
ZILLOW_API_KEY=optional_zillow_api_key
NEIGHBORHOODSCOUT_API_KEY=optional_neighborhoodscout_api_key

# JWT Security
SECRET_KEY=your_secret_key_here_change_this_in_production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# File Storage
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=50000000  # 50MB

# Environment
ENVIRONMENT=production
DEBUG=False

# CORS
ALLOWED_ORIGINS=["https://your-frontend-domain.com", "https://www.your-frontend-domain.com"]
